#include <iostream> 
using namespace std; 
int sumArray(int arr[], int size) {
int total = 0;
for (int i = 0; i < size; i++) { 
total += arr[i]; 
} 
return total;
 }
 int main() {
 int n; 
cout << "Enter number of elements: "; 
cin >> n; 
int arr[n]; 
cout << "Enter the elements: ";
 for (int i = 0; i < n; i++) {
 cin >> arr[i];
 }
 // Call the function int result = sumArray(arr, n); cout << "The sum of the array is: " << result << endl;
 return 0; 
}
